{
  "name": "@mimicprotocol/cli",
  "version": "0.0.1-rc.10",
  "license": "GPL-3.0",
  "private": false,
  "type": "commonjs",
  "bin": {
    "mimic": "./bin/run.js"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "rm -rf dist && tsc && cp -r src/templates dist/templates",
    "start": "yarn build && node ./bin/run.js",
    "test": "yarn build && ts-mocha ./tests --recursive --extension .spec.ts --exit --timeout 5000",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "@inquirer/prompts": "^7.2.4",
    "@oclif/core": "^4.2.2",
    "@oclif/plugin-not-found": "^3.2.38",
    "axios": "^1.7.9",
    "ethers": "^6.13.5",
    "form-data": "^4.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.7",
    "@types/chai": "^4.3.5",
    "@types/express": "4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.10.5",
    "axios-mock-adapter": "^2.1.0",
    "chai": "^4.3.7",
    "eslint-config-mimic": "^0.0.3",
    "mocha": "^10.2.0",
    "sinon": "^18.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "oclif": {
    "bin": "mimic",
    "commands": "./dist/commands",
    "dirname": "mimic",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-not-found"
    ]
  }
}
