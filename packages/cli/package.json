{
  "name": "@mimicprotocol/cli",
  "version": "0.0.1",
  "license": "Unlicensed",
  "private": true,
  "type": "commonjs",
  "bin": {
    "mimic": "./bin/run.js"
  },
  "scripts": {
    "postinstall": "yarn build",
    "build": "rm -rf dist && tsc && yarn copyfiles -u 1 src/templates/** dist",
    "start": "yarn build && node ./bin/run.js",
    "test": "yarn build && ts-mocha ./tests --recursive --extension .spec.ts --exit --timeout 5000",
    "lint": "eslint ."
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.4",
    "@mimic-fi/helpers": "^1.0.2",
    "@oclif/core": "^4.2.2",
    "@oclif/plugin-not-found": "^3.2.38",
    "assemblyscript": "^0.27.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.7",
    "@types/chai": "^4.3.5",
    "@types/express": "4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.10.5",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "eslint-config-mimic": "^0.0.3",
    "mocha": "^10.2.0",
    "sinon": "^18.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.0.0"
  },
  "oclif": {
    "bin": "mimic",
    "commands": "./dist/commands",
    "dirname": "mimic",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-not-found"
    ]
  }
}
